<Window x:Class="GoStore_FrontEnd.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="GoStore" Height="562" Width="1000" SnapsToDevicePixels="True" WindowStartupLocation="CenterScreen"
        xmlns:local="clr-namespace:GoStore_FrontEnd" Background="#FF1F1F1F">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="401*"/>
            <ColumnDefinition Width="591*"/>
        </Grid.ColumnDefinitions>

        <local:CommitsMap x:Name="cm1" Height="auto" Grid.Column="0" Width="auto"/>
        <Button Name="btn_test_cm" Grid.Column="1" HorizontalAlignment="Right" Height="29" Margin="0,10,10,0" VerticalAlignment="Top" Width="138" Click="btn_test_cm_Click">
            Test Commits-Map
        </Button>
    </Grid>
</Window>
